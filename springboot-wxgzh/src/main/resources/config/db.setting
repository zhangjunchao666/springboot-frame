## 打印SQL的配置
# 是否在日志中显示执行的SQL，默认false
showSql = true
# 是否格式化显示的SQL，默认false
formatSql = true
# 是否显示SQL参数，默认false
showParams = true
# 打印SQL的日志等级，默认debug
sqlLevel = debug

# 默认数据源
url = jdbc:mysql://118.190.100.101:3306/mytest?characterEncoding=UTF-8&useUnicode=true
user = root
pass = 123456



